{
    "name": "andrey-helldar/api-response",
    "description": "Package for standardizing the responses from the API of your applications.",
    "type": "library",
    "license": "MIT",
    "homepage": "http://ai-rus.com",
    "keywords": [
        "api response",
        "api-response",
        "response",
        "laravel response",
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/api-response/issues",
        "source": "https://github.com/andrey-helldar/api-response"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/http-foundation": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\ApiResponse\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\ApiResponse\\ServiceProvider"
            ]
        }
    }
}
