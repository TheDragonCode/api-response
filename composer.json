{
    "name": "dragon-code/api-response",
    "description": "Package for standardizing the responses from the API of your Symfony based applications.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "api response",
        "api-response",
        "response",
        "laravel",
        "lumen",
        "symfony",
        "api",
        "dragon-code",
        "dragon"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/api-response/issues",
        "source": "https://github.com/TheDragonCode/api-response"
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "dragon-code/contracts": "^2.2",
        "dragon-code/support": "^6.0",
        "symfony/http-foundation": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "dragon-code/laravel-support": "^3.3",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "conflict": {
        "andrey-helldar/api-response": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\ApiResponse\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "thanks": [
            {
                "name": "dragon-code/support",
                "url": "https://github.com/TheDragonCode/support"
            },
            {
                "name": "dragon-code/contracts",
                "url": "https://github.com/TheDragonCode/contracts"
            }
        ]
    }
}
